{
  "type": "module",
  "private": "true",
  "packageManager": "pnpm@10.9.0",
  "license": "MIT",
  "scripts": {
    "build": "pnpm build-vitepress",
    "build-blog": "tsx ./scripts/refreshBlogData.ts",
    "build-data": "pnpm use-data-env && pnpm use-data-env pnpm build-member && pnpm use-data-env pnpm build-blog",
    "build-member": "tsx ./scripts/refreshMemberListData.ts",
    "build-mpa": "vitepress build --mpa --base /docs/",
    "build-vitepress": "vitepress build --base /docs/",
    "build-emoji": "tsx ./scripts/emojiLoader.ts",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "commit": "git-cz",
    "dev": "vitepress",
    "lint": "eslint --cache . && pnpm lint:zh",
    "lint:eslint": "eslint --cache .",
    "lint:eslint-fix": "eslint --cache --fix .",
    "lint:md": "markdownlint **/*.md",
    "lint:zh": "zhlint **/zh/**/*.md",
    "lint:zh-fix": "zhlint **/zh/**/*.md --fix",
    "lunaria:build": "lunaria build",
    "lunaria:preview": "lunaria preview",
    "prepare": "husky",
    "serve": "vitepress serve --base /docs/",
    "site:preview": "pnpm build-blog & vitepress build --base /",
    "up": "taze major -I",
    "use-data-env": "dotenv -e .env.data -c --"
  },
