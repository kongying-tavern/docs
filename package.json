    "@iconify-json/ph": "catalog:frontend",
    "@lunariajs/core": "catalog:build",
    "@mdit/plugin-container": "catalog:markdown",
    "@mdit/plugin-demo": "catalog:markdown",
    "@mdit/plugin-figure": "catalog:markdown",
    "@mdit/plugin-img-mark": "catalog:markdown",
    "@mdit/plugin-img-size": "catalog:markdown",
    "@mdit/plugin-mark": "catalog:markdown",
    "@mdit/plugin-spoiler": "catalog:markdown",
    "@mdit/plugin-sub": "catalog:markdown",
    "@mdit/plugin-sup": "catalog:markdown",
    "@radix-icons/vue": "catalog:frontend",
    "@tiptap/core": "catalog:editor",
    "@tiptap/extension-character-count": "catalog:editor",
    "@tiptap/extension-mention": "catalog:editor",
    "@tiptap/extension-placeholder": "catalog:editor",
    "@tiptap/starter-kit": "catalog:editor",
    "@tiptap/vue-3": "catalog:editor",
    "@vueuse/core": "catalog:frontend",
    "@vueuse/integrations": "catalog:frontend",
    "@vueuse/motion": "catalog:frontend",
    "autoprefixer": "catalog:build",
    "canvaskit-wasm": "catalog:frontend",
    "class-variance-authority": "catalog:frontend",
    "clsx": "catalog:frontend",
    "date-fns": "catalog:frontend",
    "dayjs": "catalog:frontend",
    "embla-carousel-vue": "catalog:frontend",
    "feed": "catalog:frontend",
    "ky": "catalog:frontend",
    "markdown-it-attrs": "catalog:markdown",
    "markdown-it-footnote": "catalog:markdown",
    "medium-zoom": "catalog:frontend",
    "mitt": "catalog:frontend",
    "motion-v": "catalog:frontend",
    "path-to-regexp": "catalog:frontend",
    "pinia": "catalog:frontend",
    "qrcode": "catalog:frontend",
    "radix-vue": "catalog:frontend",
    "reka-ui": "catalog:frontend",
    "sass": "catalog:build",
    "tailwind-merge": "catalog:frontend",
    "thumbhash": "catalog:frontend",
    "typescript": "catalog:build",
    "unocss": "catalog:build",
    "unocss-preset-animations": "catalog:build",
    "unocss-preset-shadcn": "catalog:build",
    "vaul-vue": "catalog:frontend",
    "vitepress": "catalog:build",
    "vue": "catalog:frontend",
    "vue-request": "catalog:frontend",
    "vue-sonner": "catalog:frontend",
    "zod": "catalog:frontend"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:dev",
    "@commitlint/cli": "catalog:dev",
    "@commitlint/config-conventional": "catalog:dev",
    "@commitlint/types": "catalog:dev",
