{
  "private": true,
  "homepage": "https://yuanshen.site/docs/",
  "bugs": {
    "url": "https://github.com/jiazengp/genshinmap-docs/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jiazengp/genshinmap-docs.git",
    "branch": "master"
  },
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/genshinmap"
  },
  "license": "MIT",
  "author": " <jiazengp@gmail.com> (^_^)",
  "directories": {
    "doc": "docs"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "tsc --build",
    "commit": "npx git-cz",
    "dev": "lerna run dev --parallel",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release": "release-it --no-git.requireCleanWorkingDir",
    "lerna:bootstrap": "lerna bootstrap",
    "lerna:clean": "lerna clean",
    "lerna:list": "lerna list",
    "docs:dev": "vuepress dev docs --clean-cache",
    "docs:clean": "rimraf docs/.vuepress/.temp docs/.vuepress/.cache ./dist",
    "docs:build": "vuepress build docs --clean-cache",
    "docs:build2": "vuepress build docs",
    "docs:info": "vuepress info",
    "lint": "yarn lint:prettier --write && yarn lint:eslint --fix",
    "lint:check": "yarn lint:prettier && yarn lint:eslint",
    "lint:eslint": "eslint . --ext .js,.ts,.vue",
    "lint:md": "yarn lint:md-lint && yarn lint:md-remark && yarn lint:md-textlint",
    "lint:md-lint": "markdownlint **/*.md",
    "lint:md-remark": "remark --quiet .",
    "lint:md-textlint": "textlint ./docs/*.md",
    "lint:prettier": "prettier --check .",
    "test": "jest -i",
    "test:ui": "karma start",
    "test:coverage": "jest --coverage",
    "preview": "vite preview --port 8080 --host",
    "preview-https": "serve dist",
    "packages:check-update": "ncu --deep --timeout 600000",
    "packages:update": "ncu -u --deep --timeout 600000 && yarn install && yarn upgrade",
    "packages:doctor": "ncu --doctor -u",
    "packages:taze": "npx taze -r",
    "packages:major": "npm taze major",
    "packages:minor": "npx taze minor",
    "packages:sort": "sort-package-json",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.vue": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{md,json,yml}": [
      "prettier --write"
    ],
    "package.json": [
      "sort-package-json"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "prettier": "prettier-config-vuepress",
  "remarkConfig": {
    "plugins": [
      "frontmatter",
      [
        "footnotes",
        {
          "inlineNotes": true
        }
      ],
      "math",
      "preset-lint-consistent",
      "preset-lint-recommended",
      "preset-lint-markdown-style-guide",
      [
        "lint-maximum-line-length",
        false
      ],
      [
        "lint-list-item-spacing",
        false
      ],
      [
        "lint-list-item-indent",
        "space"
      ],
      [
        "lint-emphasis-marker",
        false
      ]
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/plugin-transform-modules-commonjs": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@textlint-rule/textlint-rule-no-unmatched-pair": "1.0.8",
    "@types/concurrently": "6.2.1",
    "@types/eslint": "7.28.0",
    "@types/jest": "26.0.24",
    "@types/katex": "0.11.1",
    "@types/markdown-it": "12.0.3",
    "@types/node": "16.4.13",
    "@types/nprogress": "0.2.0",
    "@types/qrcode": "1.4.1",
    "@types/qs": "6.9.7",
    "@types/semver": "7.3.8",
    "@types/webpack": "5.28.0",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "@vue/eslint-config-prettier": "6.0.0",
    "@vue/eslint-config-typescript": "7.0.0",
    "@vuedx/typescript-plugin-vue": "0.7.4",
    "@vuepress/plugin-debug": "2.0.0-beta.23",
    "@vuepress/plugin-docsearch": "2.0.0-beta.23",
    "@vuepress/plugin-git": "2.0.0-beta.23",
    "@vuepress/plugin-google-analytics": "^2.0.0-beta.23",
    "@vuepress/plugin-pwa": "^2.0.0-beta.23",
    "@vuepress/plugin-pwa-popup": "2.0.0-beta.23",
    "@vuepress/plugin-register-components": "^2.0.0-beta.23",
    "@vuepress/plugin-search": "^2.0.0-beta.23",
    "@vuepress/plugin-shiki": "2.0.0-beta.23",
    "@vuepress/plugin-toc": "2.0.0-beta.23",
    "@vueuse/core": "5.2.0",
    "axios": "0.21.1",
    "babel-jest": "^27.0.6",
    "concurrently": "6.2.0",
    "conventional-changelog-cli": "2.1.1",
    "cpx2": "3.0.2",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "dayjs": "1.10.6",
    "default-passive-events": "2.0.0",
    "dotenv": "10.0.0",
    "element-plus": "1.0.2-beta.70",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-vuepress": "3.2.1",
    "eslint-config-vuepress-typescript": "2.2.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-prettier-vue": "3.1.0",
    "https-localhost": "4.6.5",
    "husky": "7.0.1",
    "jest": "^27.0.6",
