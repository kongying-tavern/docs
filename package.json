    "js-md5": "0.7.3",
    "katex": "0.13.13",
    "lerna": "^4.0.0",
    "lint-md": "0.2.0",
    "lint-staged": "11.1.2",
    "markdownlint-cli": "0.28.1",
    "mocha": "9.0.3",
    "npm-check-updates": "11.8.3",
    "prettier": "2.3.2",
    "prettier-config-vuepress": "1.2.0",
    "qrcode": "1.4.4",
