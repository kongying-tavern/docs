    "depcheck": "catalog:dev",
    "dotenv-cli": "catalog:dev",
    "eslint": "catalog:dev",
    "fontaine": "catalog:dev",
    "git-cz": "catalog:dev",
    "husky": "catalog:dev",
    "js-yaml": "catalog:utils",
    "lint-staged": "catalog:dev",
    "lodash-es": "catalog:frontend",
    "markdown-it": "catalog:markdown",
    "markdown-it-container": "catalog:markdown",
    "markdown-it-kbd-better": "catalog:markdown",
