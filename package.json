    "@iconify-json/lucide": "^1.2.30",
    "@iconify-json/mdi": "^1.2.3",
    "@iconify-json/ph": "^1.2.2",
    "@lunariajs/core": "^0.1.1",
    "@mdit/plugin-container": "^0.16.0",
    "@mdit/plugin-figure": "^0.16.0",
    "@mdit/plugin-img-mark": "^0.16.0",
    "@mdit/plugin-img-size": "^0.16.0",
    "@mdit/plugin-mark": "^0.16.0",
    "@mdit/plugin-spoiler": "^0.16.0",
    "@mdit/plugin-sub": "^0.16.0",
    "@mdit/plugin-sup": "^0.16.0",
    "@radix-icons/vue": "^1.0.0",
    "@tiptap/core": "^2.11.5",
    "@tiptap/extension-character-count": "^2.11.5",
    "@tiptap/extension-mention": "^2.11.5",
    "@tiptap/starter-kit": "^2.11.5",
    "@tiptap/vue-3": "^2.11.5",
    "@types/js-yaml": "^4.0.9",
    "@unhead/vue": "^1.11.20",
    "@vue/shared": "^3.5.13",
    "@vueuse/core": "^13.0.0",
    "@vueuse/integrations": "^13.0.0",
    "@vueuse/motion": "^3.0.3",
    "autoprefixer": "^10.4.21",
    "canvaskit-wasm": "^0.39.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "compressorjs": "^1.2.1",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.13",
    "embla-carousel-vue": "^8.5.2",
    "feed": "^4.2.2",
    "js-yaml": "^4.1.0",
    "ky": "^1.7.5",
    "markdown-it-attrs": "^4.3.1",
    "markdown-it-footnote": "^4.0.0",
    "medium-zoom": "^1.1.0",
    "motion-v": "^0.13.0",
    "pinia": "^3.0.1",
    "qrcode": "^1.5.4",
    "radix-vue": "^1.9.17",
    "reka-ui": "^2.1.1",
    "sass": "^1.85.1",
    "tailwind-merge": "^3.0.2",
    "thumbhash": "^0.1.1",
    "typescript": "^5.8.2",
    "ua-parser-js": "^2.0.3",
    "unocss": "^66.0.0",
    "unocss-preset-animations": "^1.1.1",
    "unocss-preset-shadcn": "^0.5.0",
    "vaul-vue": "^0.4.1",
    "vitepress": "1.6.3",
    "vue": "^3.5.13",
    "vue-request": "^2.0.4",
    "vue-sonner": "^1.3.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.10.1",
    "@commitlint/cli": "19.6.1",
    "@commitlint/config-conventional": "19.6.0",
    "@commitlint/types": "19.5.0",
    "@nolebase/markdown-it-unlazy-img": "^2.15.1",
    "@nolebase/vitepress-plugin-thumbnail-hash": "^2.15.1",
    "@types/crypto-js": "4.2.2",
    "@types/lodash-es": "4.17.12",
    "@types/markdown-it": "14.1.2",
    "@types/markdown-it-attrs": "^4.1.3",
    "@types/markdown-it-container": "^2.0.10",
    "@types/markdown-it-footnote": "3.0.4",
    "@types/node": "22.10.2",
    "@types/ua-parser-js": "0.7.39",
    "@unlazy/vue": "^0.12.3",
    "@unocss/eslint-config": "^66.0.0",
    "@unocss/eslint-plugin": "^66.0.0",
    "@unocss/extractor-mdc": "0.62.4",
    "@unocss/preset-tagify": "0.64.1",
    "conventional-changelog-cli": "5.0.0",
    "cz-conventional-changelog": "3.3.0",
    "dotenv-cli": "8.0.0",
    "eslint": "^9.22.0",
    "fontaine": "0.5.0",
    "git-cz": "4.9.0",
    "husky": "9.1.7",
    "lint-staged": "15.3.0",
    "lodash-es": "4.17.21",
    "markdown-it": "14.1.0",
    "markdown-it-container": "4.0.0",
    "markdown-it-kbd-better": "1.2.5",
    "markdownlint-cli": "0.41.0",
    "open-editor": "^5.1.0",
    "pnpm": "10.6.3",
    "sort-package-json": "2.12.0",
    "stylelint": "16.12.0",
    "stylelint-config-standard": "36.0.1",
    "stylelint-config-standard-scss": "13.1.0",
    "stylelint-order": "6.0.4",
    "taze": "0.16.9",
    "tsconfig-paths": "4.2.0",
    "tsconfig-vuepress": "5.2.1",
    "tsx": "4.19.2",
    "unlazy": "^0.12.3",
    "vite": "^6.2.2",
    "vite-plugin-inspect": "0.8.9",
    "vitepress-plugin-llms": "^1.0.0",
    "zhlint": "0.8.2"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "@parcel/watcher",
      "esbuild",
      "vue-demi"
    ]
  }
}
