    "vuepress-plugin-photo-swipe": "2.0.0-alpha.6",
    "vuepress-plugin-sass-palette": "2.0.0-alpha.6",
    "vuepress-plugin-seo2": "2.0.0-alpha.6",
    "vuepress-plugin-sitemap2": "2.0.0-alpha.6",
    "vuepress-vite": "2.0.0-beta.23",
    "yaml": "2.0.0-7"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
